{
    "name": "alexeikadev/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2 (AlexeiKaDev fork)",
    "keywords": ["yii2", "yii2-user", "user management", "alexeikadev"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/AlexeiKaDev/yii2-user/issues",
        "forum": "", 
        "source": "https://github.com/AlexeiKaDev/yii2-user"
    },
    "authors": [
        {
            "name": "Dmitry Erofeev",
            "email": "dmeroff@gmail.com",
            "homepage": "http://dmeroff.ru"
        },
        {
            "name": "AlexeiKaDev",
            "email": "alexei.ka.dev@example.com", 
            "homepage": "https://github.com/AlexeiKaDev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2 <8.5",
        "yiisoft/yii2": "^2.0.40",
        "yiisoft/yii2-symfonymailer": "^2.0.3|^3.0|^4.0",
        "yiisoft/yii2-authclient": "^2.1.0|^2.2|^3.0",
        "yiisoft/yii2-bootstrap5": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "AlexeiKaDev\\Yii2User\\": ""
        }
    },
    "extra": {
        "bootstrap": "AlexeiKaDev\\Yii2User\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0.x-dev", 
            "dev-develop": "1.1.x-dev"
        }
    }
}
